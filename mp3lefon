//MP3lefon, inspirert av gamledager og av Chris Borge pÃ¥ youtube.
differanse=12.5;
detail=70;
telefon(0,1,0);
//jerndeksel();
//translate([80,0,25.5])
//rotate([-8,0,90])
//kilesprekkroer();

//jafslokk();
//screws();
//translate([0,0,-50])kile();
//pads(1);
//knapper();
//translate([00,0,0])knapphull();
//lokk();
//hoyttaler();
